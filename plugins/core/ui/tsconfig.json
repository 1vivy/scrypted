{
    "compilerOptions": {
      "typeRoots": [
        "./node_modules/@types",
        "./node_modules/vuetify/types"
      ],
      "target": "es2017",
      "module": "esnext",
      "jsx": "preserve",
      "importHelpers": true,
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "baseUrl": ".",
      "types": [
        "node",
        "webpack-env",
        "vuetify",
        "dom-webcodecs",
      ],
      "lib": [
        "esnext",
        "dom",
        "dom.iterable",
        "scripthost"
      ]
    },
    "exclude": [
      "node_modules"
    ]
  }
  